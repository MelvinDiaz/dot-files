[
  {
    "context": "Editor && vim_mode == insert && !menu",
    "bindings": {
      "j k": ["vim::SwitchMode", "Normal"]
    }
  },
  {
    "context": "Editor ",
    "bindings": {
      "ctrl-n": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-n": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "ProjectPanel && not_editing",
    "bindings": {
      "ctrl-shift-e": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "ctrl-n": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-i": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-i": "terminal_panel::ToggleFocus"
    }
  }
]
